import java.util.*;
class Vote
{
public static void main(String arg[])
{
Scanner in=new Scanner(System.in);
int age;
System.out.println("enter the age of the voter");
age=in.nextInt();
{
if (age<=18)
System.out.println("voter is not allowed to vote");
else 
System.out.println("voter is allowed to vote");
}
}
}
